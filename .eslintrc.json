{
    "globals":{
        "sap": false,
        "jQuery": false,
        "module": false
        },
    "rules": {
        "no-console": 0,
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "eslint:recommended"
}

